{
  "name": "menually",
  "version": "1.1.0",
  "description": "A utility package to enable developers create keyboard accessible menus.",
  "main": "dist/menually.esm.min.js",
  "module": "dist/menually.esm.min.js",
  "browser": "dist/menually.umd.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts",
    "complile": "npx tsc",
    "prepare": "husky install",
    "commit": "cz",
    "build": "rollup -c",
    "watch": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Menuable/MenuAlly.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Menuable/MenuAlly/issues"
  },
  "homepage": "https://github.com/Menuable/MenuAlly#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "lint-staged": {
    "*.{css,less,scss,html,ts}": [
      "prettier --write ."
    ],
    "*.js": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^22.0.10",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "eslint-config-standard-with-typescript": "^43.0.0"
  }
}
